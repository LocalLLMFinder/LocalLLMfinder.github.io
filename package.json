{
  "name": "gguf-model-index",
  "version": "1.0.0",
  "description": "A modern, minimal, and responsive website for browsing GGUF models",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:analyze": "vite build && node scripts/optimize-build.js",
    "preview": "vite preview",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:search": "vitest --run tests/search.test.js",
    "test:integration": "vitest --run tests/integration.test.js",
    "test:e2e": "vitest --run tests/e2e.test.js",
    "test:all": "vitest --run tests/",
    "deploy:monitor": "bash scripts/monitor-deployment.sh"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.0",
    "jsdom": "^25.0.1",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.4.0",
    "vite": "^5.0.0",
    "vitest": "^3.2.4"
  },
  "keywords": [
    "gguf",
    "models",
    "ai",
    "machine-learning"
  ],
  "author": "GGUF Loader Team",
  "license": "MIT"
}
